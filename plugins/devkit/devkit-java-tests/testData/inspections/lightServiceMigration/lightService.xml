<idea-plugin>
  <id>com.example.demo</id>

  <extensionPoints>
    <extensionPoint qualifiedName="com.intellij.applicationService" beanClass="com.intellij.openapi.components.ServiceDescriptor"/>
  </extensionPoints>
  <extensions defaultExtensionNs="com.intellij">
    <<warning descr="Service class annotated with '@Service' does not need to be registered in plugin.xml">applicationService</warning> serviceImplementation="com.example.demo.MyService"/>
  </extensions>
</idea-plugin>
