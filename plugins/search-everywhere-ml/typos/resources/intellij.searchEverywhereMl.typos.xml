<idea-plugin package="com.intellij.ide.actions.searcheverywhere.ml.typos">

  <resource-bundle>messages.searchEverywhereMlTyposBundle</resource-bundle>

  <extensions defaultExtensionNs="com.intellij">
    <backgroundPostStartupActivity implementation="com.intellij.ide.actions.searcheverywhere.ml.typos.ActionsLanguageModel$ModelStartupActivity"/>

    <searchEverywhereSpellingCorrector
      implementation="com.intellij.ide.actions.searcheverywhere.ml.typos.SearchEverywhereSpellingCorrectorFactoryImpl" />

    <registryKey key="search.everywhere.ml.typos.min.confidence"
                 description="Minimum confidence required for spelling suggestion"
                 defaultValue="0.5"/>

    <registryKey key="search.everywhere.ml.typos.show.confidence"
                 description="Show confidence in Search Everywhere"
                 defaultValue="false"/>
  </extensions>
</idea-plugin>